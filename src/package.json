{
  "name": "StepWise",
  "version": "0.0.1",
  "main": "main.js",
  "description": "Screenshots wise steps recording app.",
  "scripts": {
    "start": "electron ."
  },
  "repository": "https://github.com/yogeshjog/StepWise",
  "keywords": [
    "Electron",
    "StepWise",
    "ScreenShots",
    "Capture Screen",
    "Record"
  ],
  "author": "Yogesh Jog",
  "license": "MIT",
  "devDependencies": {
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0"
  },
  "iohook": {
    "targets": [
      "node-64",
      "electron-69"
    ],
    "platforms": [
      "win32",
      "darwin",
      "linux"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  },
  "builds": {
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "createDesktopShortcut": true,
      "installerIcon": "build/icon.png",
      "uninstallerIcon": "build/icon.png",
      "uninstallDisplayName": "Uninstall StepWise",
      "license": "EULA"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.png",
      "publisherName": "Yogesh Jog"
    },
    "buildVersion": "0.1"
  },
  "dependencies": {
    "iohook": "^0.4.4",
    "screenshot-desktop": "^1.7.0"
  }
}
